generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measure {
  id              Int       @id @default(autoincrement())
  customer_code   String
  measure_datetime DateTime
  measure_type    String
  measure_value   Int
  image_url       String
  measure_uuid    String
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
}

